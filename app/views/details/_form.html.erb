<%= form_for([current_account, set_project, @detail]) do |f| %>
  <% f.hidden_field :project_id %>
  <li><%= f.text_field :name, placeholder: "name" %></li>
  <br>
  <li><%= f.text_field :description, placeholder: "description" %></li>
  <br>
  <%= f.button "add detail", class: "btn btn-success" %>
<% end %>
