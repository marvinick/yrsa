<%= link_to account_project_path(current_account, set_project) do %>
  <div class="formy-back">
    <%= set_project.title %>
  </div>
<% end %>
<div class="formy">
  <div class="row">
    <div class="my-details">
      <%= render @details %>
    </div>
    <div class="row">
      <%= link_to "add", new_account_project_detail_path(current_account, @project, @detail), remote: true, class: "new-detail-button btn btn-default pull-right" %>
    </div>
  </div>
</div>
