<h2><%= @account.name %></h2>
<% @account.projects.each do |project| %>
<div class="col s12 m">
  <div class="row">

      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= link_to project.title, account_project_path(@account, project) %></span>
          <p><%= project.description %></p>
        </div>
      </div>
    </div>
  </div>
<% end %>
<div class="card-action">
  <a href="#"></a>
  <a href="#"><%= link_to "Users", account_users_path(@account, @users) %></a>
  <a href="#"><%= link_to "Invitations", account_invitations_path(@account, @invitations) %></a>
  <a href="#"><%= link_to "New Project", new_account_project_path(@account) %></a>
</div>
