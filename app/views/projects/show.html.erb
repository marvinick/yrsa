<% @title= @project.title %>

<div class="formy">
	<div class="container-fluid">
		<div class="row">
			<%= render "header" %>
		</div>
		<br>
		<div class="row">
			<div class="col-md-1"></div>
			<div class="col-md-3">
				<%= render "stats" %>
			</div>
			<div class="col-md-3">
				<%= render "users" %>
			</div>
			<div class="col-md-5">
				<%= render "details" %>
			</div>
		</div>
	</div>
</div>

<br>

<div class="container-fluid">
	<div class="row">
		<%= render "actions" %>
	</div>
	<div class="row">
		<div id="cards" class="tabcontent">
			<%= render "cards" %>
		</div>
		<div id="lists" class="tabcontent">
			<%= render "items_list" %>
		</div>
		<div id="simple-lists" class="tabcontent">
			<%= render "simple_lists" %>
		</div>
	</div>
</div>
<br>

<div class="container">

</div>


