<%= form_for([current_account, set_project, set_item, @detail, @review]) do |f| %>
  <%= f.hidden_field :detail_id %>
  <div class="control">
    <%= f.text_field :description, placeholder: 'name', label: false, class: "input is-medium" %>
  </div>
  <br>
  <div class="control">
    <%= f.text_field :value, placeholder: 'value', label: false, class: "input is-medium"  %>
  </div>
  <br>
  <%= f.button :submit, class: "button is-primary" %>
<% end %>
